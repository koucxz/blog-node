menu-fsize = 20px

.header{
  background-color: #fff;
  padding: 10px;
  overflow: hidden;
  border-bottom:1px solid #ccc;
}
.title{
  font-size: 30px;
  color: #333;
  display: inline-block;
  padding-left: 0.3rem;
}
:global(.ant-menu){
  border-bottom none
}
:global(.ant-menu-item) {
  font-size menu-fsize
  padding 0 20px
}
:global(.ant-menu-item .anticon) {
  font-size menu-fsize
}
