.bread-div {
  padding 5px
  border-bottom 1px solid #eee
  background-color #e1f0ff
}

.detailed-title {
  font-size 18px
  text-align center
  padding 10px
}

.center {
  text-align center
}

.detailed-content {
  padding 14px
}

code {
  display block
  background-color #f3f3f3
  padding 5px
  overflow-y auto
  font-weight 300
  font-family Menlo, monospace
  border-radius 4px
}

.title-anchor {
  color #888
  padding 4px
  margin 0
  height auto
  line-height 16px
  border-bottom 1px dashed #eee
}

.active {
  color rgb(30, 144, 255)
}

.nav-title {
  text-align center
  color #888
  border-bottom 1px solid rgb(30, 144, 255)
}
